@model ExpressForms.Filters.ExpressFormsFilterNumber
@{
    Layout = "~/Views/Shared/ExpressFormsFilters/_FilterLayout.cshtml";
}

@{string divId = "divFilter_" + Model.FilterName; }

<div id='@divId' >
    Minimum
    @Html.TextBox(Model.FilterName + "MinDate", "", new { @class = "ExpressFormsFilterNumberMin" })
    Maximum
    @Html.TextBox(Model.FilterName + "MaxDate", "", new { @class = "ExpressFormsFilterNumberMax" })
</div>

<script>
    $(document).ready(function () {
        $('#@divId').find('input').change(ef.filterCriteriaChanged);
    });
</script>